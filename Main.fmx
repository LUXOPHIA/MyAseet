                  ''
                  
                    '    float4 CD = ( _AmbiLight + _Light.Col * D ) * ( _DiffRatio *' +
                    ' DP );          //'#25313#25955#21453#23556#20809
                  ''
                  
                    '    //----------------------------------------------------------' +
                    '----------------'
                  ''
                  
                    '    float S = pow( max( dot( N, H ), 0.0 ), _SpecShiny );       ' +
                    '                //'#20809#32218#37857#38754#21453#23556#29575
                  ''
                  
                    '    float4 CS = ( _Light.Col * S ) * _SpecRatio;                ' +
                    '                //'#20809#32218#37857#38754#21453#23556#20809
                  ''
                  
                    '    //----------------------------------------------------------' +
                    '----------------'
                  ''
                  
                    '    float3 E = normalize( reflect( -V, N ) );                   ' +
                    '                //'#35222#32218#37857#38754#21453#23556#26041#21521
                  ''
                  
                    '    float4 CE = _EnviImage.Sample( _SamplerState, VectorToSky( E' +
                    ' ) );           //'#29872#22659#37857#38754#21453#23556#20809
                  ''
                  
                    '    //----------------------------------------------------------' +
                    '----------------'
                  ''
                  
                    '    TColorVec3 A = Refract3( V, N, _RefrIndex );                ' +
                    '                //'#35222#32218#37857#38754#23624#25240#26041#21521#215'RGB'
                  ''
                  
                    '    float4 CA = Sample3( A );                                   ' +
                    '                //'#29872#22659#37857#38754#23624#25240#20809
                  ''
                  
                    '    //----------------------------------------------------------' +
                    '----------------'
                  ''
                  
                    '    float4 CI = _EmisLight + ( CA - CD ) * _TranRatio + CD;     ' +
                    '                //'#20869#37096#20809
                  ''
                  
                    '    float4 CO = CE + CS;                                        ' +
                    '                //'#37857#38754#21453#23556#20809
                  ''
                  
                    '    float4 F = Fresnel4( V, N, _RefrIndex );                    ' +
                    '                //'#35222#32218#37857#38754#21453#23556#29575
                  ''
                  '    F = clamp( F, 0, 1 );'
                  ''
                  '    _Result.Col = ( CO - CI ) * F + CI;'
                  ''
                  
                    '    //----------------------------------------------------------' +
                    '----------------'
                  ''
                  '    _Result.Col.a = 1;'
                  ''
                  '    _Result.Col = _Opacity * _Result.Col;'
                  ''
                  '    return _Result;'
                  '}'
                  ''
                  
                    '//##############################################################' +
